        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Only Approved Design Wins can be set to stage &quot;Commitment&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Competitor_required_for_LostCanc_Oppty</fullName>
        <active>true</active>
        <errorConditionFormula>AND
 (
  OR
   (
    TEXT(StageName)=&quot;Lost&quot;, 
    TEXT(StageName)=&quot;Cancelled&quot;
   ),
   ISBLANK(TEXT(Competitors__c)),
   RecordType.DeveloperName = &quot;SFDC_Oppty&quot;,
   NOT
   (
    OR($User.ProfileId=&quot;00e20000001Nc7g&quot;,
       $User.ProfileId=&quot;00eD0000001oBDw&quot;)
   )
 )</errorConditionFormula>
        <errorDisplayField>Competitors__c</errorDisplayField>
        <errorMessage>Competitor should be selected for Lost/Cancelled Opportunity</errorMessage>
    </validationRules>
    <validationRules>
