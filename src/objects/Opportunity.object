    <fields>
        <fullName>Automotive_Segment_Mismatch__c</fullName>
        <description>This field is part of Opportunity/Opportunity Product fields phase-1 deletion. But unable to delete this fields as we are unable to delete the inactive approval processes in which this field is referenced as data is associated with them. So just renaming the field label to obsolete this field.</description>
        <externalId>false</externalId>
        <formula>IF(
  OR(
    ISPICKVAL(
      Industry_Segment__c , 
      &quot;Automotive &amp; Transportation&quot; 
    ) ,
    ISPICKVAL(
      Account.Parent.Industry_Segment__c , 
      &quot;Automotive &amp; Transportation&quot; 
    )
  ) ,
  IF( 
    AND(
      Parent_Account_Segment_Approval__c ,
      ISPICKVAL(
        Industry_Segment__c , 
        &quot;Automotive &amp; Transportation&quot; 
      ) 
    ) ,
    0 ,
    1 
  ) ,
  0
)</formula>
