    <fields>
        <fullName>Projection__c</fullName>
        <description>1608 Release - SIR 1018.</description>
        <externalId>false</externalId>
        <formula>IF( 
	ISPICKVAL( Design_Win_Approval_Process__c , &quot;Approved&quot; ), &quot;Approved&quot;, 
	IF( 
	ISPICKVAL( Design_Win_Approval_Process__c , &quot;Pending&quot; ), &quot;Pending&quot;, 
	IF( 
	ISPICKVAL( Design_Win_Approval_Process__c , &quot;Lost&quot; ), &quot;Lost&quot;, 
        IF( ECD_Quarter__c - Today_Quarter__c &lt; 0 , &quot;Past Due&quot;,
	IF( 
	ECD_Quarter__c - Today_Quarter__c &gt;1, &quot;Undetermined&quot;, 
	IF( 
	ISPICKVAL( Probability_to_Win__c , &quot;0% - 50%&quot;), &quot;Unlikely&quot;, 
	IF( 
	ISPICKVAL( Probability_to_Win__c , &quot;80% - 100%&quot;), &quot;Most Likely&quot;, 
	IF( 
	AND( 
	ISPICKVAL( Probability_to_Win__c , &quot;50% - 80%&quot;), 
	ECD_Quarter__c - Today_Quarter__c = 1, 
	ECD_Months_of_Quarter__c = 3 
	), &quot;Most Likely&quot;, 
	IF( 
	AND( 
	ISPICKVAL( Probability_to_Win__c , &quot;50% - 80%&quot;), 
	ECD_Quarter__c - Today_Quarter__c = 1, 
	ECD_Months_of_Quarter__c &lt; 3 
	), &quot;Unlikely&quot;, 
	IF( 
	AND( 
	ISPICKVAL( Probability_to_Win__c , &quot;50% - 80%&quot;), 
	ECD_Quarter__c - Today_Quarter__c = 0, 
	Today_Months_of_Quarter__c = 3 
	), &quot;Unlikely&quot;, 
	IF( 
	AND( 
	ISPICKVAL( Probability_to_Win__c , &quot;50% - 80%&quot;), 
	ECD_Quarter__c - Today_Quarter__c = 0, 
	Today_Months_of_Quarter__c &lt; 3 
	), &quot;Most Likely&quot;, 
	&quot;Undetermined&quot; 
	) 
	) 
	) 
	) 
	) 
	) 
	) 
	) 
	) 
	)
)</formula>
        <label>Projection</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
