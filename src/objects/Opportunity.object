    <validationRules>
        <fullName>Close_Date_vs_Production_date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
CloseDate &gt; Production_Date__c, 
NOT(
OR(
$User.ProfileId=&quot;00e20000001Nc7g&quot;,
$User.ProfileId=&quot;00eD0000001oBDw&quot;
)
)
)</errorConditionFormula>
