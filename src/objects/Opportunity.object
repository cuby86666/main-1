    <validationRules>
        <fullName>Close_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  OR(
    ISNEW(), 
    ISCHANGED( CloseDate ),
    AND(
      ISCHANGED( Design_Win_Approval_Process__c ),
      ISPICKVAL( Design_Win_Approval_Process__c, &quot;Open&quot; )
    )
  ),
  CloseDate &lt; TODAY() ,
  NOT(
    OR(
      $User.ProfileId=&quot;00e20000001Nc7g&quot;,$User.ProfileId=&quot;00eD0000001oBDw&quot;
    )
  )
)</errorConditionFormula>
        <errorDisplayField>CloseDate</errorDisplayField>
        <errorMessage>Close Date can not be prior to today. Please select a new Close Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_vs_Production_date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
CloseDate &gt; Production_Date__c, 
NOT(
OR(
$User.ProfileId=&quot;00e20000001Nc7g&quot;,
$User.ProfileId=&quot;00eD0000001oBDw&quot;
)
)
)</errorConditionFormula>
        <errorMessage>Expected Close Date should not be later than the Production Date. Please select a new date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Commitment_Only_for_Approved_Design_Wins</fullName>
