    </validationRules>
    <validationRules>
        <fullName>LostReasonRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
OR
(
TEXT(StageName) = &quot;Lost&quot;, 
TEXT(StageName) = &quot;Cancelled&quot;
),
ISBLANK(TEXT(Lost_Reason__c)),
RecordType.DeveloperName = &quot;SFDC_Oppty&quot;,
