        <externalId>false</externalId>
        <formula>IF(AND(OR(TEXT(Industry_Segment__c) =&quot;Automotive&quot;, TEXT(Industry_Segment__c) =&quot;Healthcare&quot;),
OR(YEAR(Production_Date__c )=2010,Production_Date__c=DATE(2011,01,01))), X2011_Value__c+ X2012_Value__c+X2013_Value__c+X2014_Value__c, 
IF(AND(OR(TEXT(Industry_Segment__c) &lt;&gt;&quot;Automotive&quot;, TEXT(Industry_Segment__c)&lt;&gt;&quot;Healthcare&quot;), 
OR(YEAR(Production_Date__c )=2010,Production_Date__c=DATE(2011,01,01))), 
X2011_Value__c+ X2012_Value__c+X2013_Value__c, 
IF(AND(OR(TEXT(Industry_Segment__c) =&quot;Automotive&quot;, TEXT(Industry_Segment__c) =&quot;Healthcare&quot;),
OR(YEAR(Production_Date__c )=2011,Production_Date__c=DATE(2012,01,01))), 
X2012_Value__c+ X2013_Value__c+X2014_Value__c+X2015_Value__c, 
IF(AND(OR( TEXT(Industry_Segment__c) &lt;&gt;&quot;Automotive&quot;, TEXT(Industry_Segment__c)&lt;&gt;&quot;Healthcare&quot;), 
OR(YEAR(Production_Date__c )=2011,Production_Date__c=DATE(2012,01,01))), 
X2012_Value__c+ X2013_Value__c+X2014_Value__c, 
IF(AND(OR(TEXT(Industry_Segment__c) =&quot;Automotive&quot;, TEXT(Industry_Segment__c) =&quot;Healthcare&quot;),
OR(YEAR(Production_Date__c )=2012,Production_Date__c=DATE(2013,01,01))), 
X2013_Value__c+ X2014_Value__c+X2015_Value__c+X2016_Value__c, 
IF(AND(OR( TEXT(Industry_Segment__c) &lt;&gt;&quot;Automotive&quot;, TEXT(Industry_Segment__c)&lt;&gt;&quot;Healthcare&quot;), 
OR(YEAR(Production_Date__c )=2012,Production_Date__c=DATE(2013,01,01))), 
X2013_Value__c+ X2014_Value__c+X2015_Value__c, 
IF(AND(OR(TEXT(Industry_Segment__c) =&quot;Automotive&quot;, TEXT(Industry_Segment__c) =&quot;Healthcare&quot;),
OR(YEAR(Production_Date__c )=2013,Production_Date__c=DATE(2014,01,01))), 
X2014_Value__c+ X2015_Value__c+X2016_Value__c+X2017_Value__c, 
IF(AND(OR( TEXT(Industry_Segment__c) &lt;&gt;&quot;Automotive&quot;, TEXT(Industry_Segment__c)&lt;&gt;&quot;Healthcare&quot;), 
OR(YEAR(Production_Date__c )=2013,Production_Date__c=DATE(2014,01,01))), 
X2014_Value__c+ X2015_Value__c+X2016_Value__c, 
IF(AND(OR(TEXT(Industry_Segment__c) =&quot;Automotive&quot;, TEXT(Industry_Segment__c) =&quot;Healthcare&quot;),
OR(YEAR(Production_Date__c )=2014,Production_Date__c=DATE(2015,01,01))), 
X2015_Value__c+ X2016_Value__c+X2017_Value__c+X2018_Value__c, 
IF(AND(OR( TEXT(Industry_Segment__c) &lt;&gt;&quot;Automotive&quot;, TEXT(Industry_Segment__c)&lt;&gt;&quot;Healthcare&quot;), 
OR(YEAR(Production_Date__c )=2014,Production_Date__c=DATE(2015,01,01))), 
X2015_Value__c+ X2016_Value__c+X2017_Value__c, 
IF(AND(OR(TEXT(Industry_Segment__c) =&quot;Automotive&quot;, TEXT(Industry_Segment__c) =&quot;Healthcare&quot;),
OR(YEAR(Production_Date__c )=2015,Production_Date__c=DATE(2016,01,01))), 
X2016_Value__c+ X2017_Value__c+X2018_Value__c+X2019_Value__c, 
